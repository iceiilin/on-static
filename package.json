    "postinstall": "./download.sh",
    "test": "./node_modules/.bin/mocha $(find spec -name '*-spec.js') -R spec --require spec/helper.js",
    "debug": "./node_modules/.bin/mocha debug $(find spec -name '*-spec.js') -R spec --require spec/helper.js",
    "dev": "cd gui; node build/dev-server.js",
    "build": "cd gui; node build/build.js; sudo rm -rf ../static/gui; sudo cp -r dist/ ../static/gui"
